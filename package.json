{
  "name": "shmobx",
  "version": "1.0.0",
  "description": "Find stylelint rules that you don't have in your config",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "base-prettier": "prettier --ignore-path .gitignore",
    "base-eslint": "eslint --ignore-path .gitignore",
    "format": "npm run base-eslint -- --fix \"**/*.js\" && npm run base-prettier -- --write \"**/*.js\"",
    "lint-prettier": "npm run base-prettier -- -l \"**/*.js\"",
    "lint-eslint": "npm run base-eslint -- --rule 'no-console: 1' --max-warnings 0 \"**/*.js\"",
    "lint": "npm run lint-prettier && npm run lint-eslint",
    "test": "jest ./src",
    "test-watch": "npm run test -- --watch --notify",
    "test-coverage": "npm run test -- --coverage",
    "test-ci": "npm run test-coverage && npm run test-e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run base-eslint -- --fix --max-warnings 0",
      "npm run base-prettier -- --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.11.1",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-jest": "22.1.2",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "prettier": "1.15.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexilyaev/shmobx.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexilyaev/shmobx/issues"
  },
  "homepage": "https://github.com/alexilyaev/shmobx#readme"
}
